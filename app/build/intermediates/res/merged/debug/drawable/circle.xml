<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape android:shape="oval" >
            <solid android:color="#7DC243" />

            <padding
                android:bottom="3dp"
                android:left="3dp"
                android:right="3dp"
                android:top="3dp" />

            <corners android:radius="200dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval" >
            <solid android:color="#FFF" />

            <padding
                android:bottom="8dp"
                android:left="8dp"
                android:right="8dp"
                android:top="8dp" />

            <corners android:radius="200dp" />
        </shape>
    </item>

</layer-list>